{
  "name": "node-express-ts",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "node",
    "express",
    "typescript",
    "tslint",
    "prettier",
    "airbnb",
    "husky"
  ],
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=production node ./build/index.js",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "tslint": "tslint --project tsconfig.json",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:write && npm run tslint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fazzacoot/PersonalWebsite.git"
  },
  "author": "jorgemcdev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Fazzacoot/PersonalWebsite.git/issues"
  },
  "homepage": "https://github.com/Fazzacoot/PersonalWebsite.git#readme",
  "devDependencies": {
    "@types/express": "^4.17.0",
    "husky": "^3.0.1",
    "prettier": "^1.18.2",
    "ts-node-dev": "^1.0.0-pre.40",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "pug": "^3.0.1",
    "typed.js": "^2.0.11"
  }
}
